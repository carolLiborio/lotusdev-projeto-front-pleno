{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "plugin:react/recommended"
      // "prettier",
      // "prettier/react",
      // "airbnb"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      // "prettier",
      "react",
      "react-hooks"
  ],
  "rules": {
    "no-unused-vars": "warn",
    // "prettier/prettier": "error",
    "react/jsx-filename-extension": 0,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "import/prefer-default-export": "off",
    "camelcase": "off",
    "react/prop-types": 0
  }
}
